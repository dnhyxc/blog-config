<footer id="footer" style="text-shadow: none;">
  <div class="outer">
    <div id="footer-info">
      <span class="footer-left">
        &copy; <%= date(new Date(), 'YYYY') %> <%= config.author || config.title %>
      </span>
      <% if (theme.busuanzi && theme.busuanzi.enable){ %>
      <!-- 不蒜子统计 -->
      <span id="busuanzi_container_site_pv"><i class=" fa fa-spinner"></i>本站总访问量<span
          id="busuanzi_value_site_pv"></span>次 |
        <span id="busuanzi_container_site_uv""><i class=" fa fa-user-md"></i>本站总访客数<span
            id="busuanzi_value_site_uv"></span>人
          <script async src="//busuanzi.ibruce.info/busuanzi/2.3/busuanzi.pure.mini.js"></script>
          <% } %>
        </span>
      </span>
      <div class="footer-right">
        <a href="dnhyxc.github.io" target="_blank">dnhyxc</a> blog make by <a href="https://github.com/dnhyxc"
          target="_blank">ninghai</a>
      </div>
    </div>
  </div>
</footer>